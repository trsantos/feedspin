<% provide(:title, "New feed") %>
<header>
    <h2>New feed</h2>
</header>

<div class="row">
    <div class="medium-8 medium-offset-2">
	<%= form_for(:feed, url: feeds_path) do |f| %>
	    <%= f.label :feed %>
	    <%= f.text_field :feed_url %>

	    <%= f.submit "Submit", class: "button expand" %>
	<% end %>
    </div>
</div>
